{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": false,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.ts",
    "css": "./src/index.css",
    "baseColor": "neutral",
    "cssVariables": true,
    "prefix": ""
  },
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils",
    "ui": "@/components/ui",
    "lib": "@/lib",
    "hooks": "@/hooks"
  },
  "paths": {
    "components/*": [
      "./src/components/*"
    ],
    "utils/*": [
      "./src/lib/utils/*"
    ],
    "ui/*": [
      "./src/components/ui/*"
    ],
    "lib/*": [
      "./src/lib/*"
    ],
    "hooks/*": [
      "./src/hooks/*"
    ],
    "@/*": [
      "./src/*"
    ]
  }
}